{
  "name": "alomerry-blog",
  "private": true,
  "description": "Alomerry Wu's blog, powered by VuePress 2, themed by Gungnir.",
  "license": "Apache-2.0",
  "author": "Alomerry Wu <alomerry@hotmail.com>",
  "scripts": {
    "clean": "rimraf blog/.vuepress/.temp blog/.vuepress/.cache dist",
    "dev": "vuepress-cli dev blog --clean-cache",
    "build": "pnpm clean && vuepress-cli build blog --clean-cache",
    "serve": "serve blog/.vuepress/dist"
  },
  "devDependencies": {
    "@vuepress/bundler-vite": "2.0.0-beta.48",
    "@vuepress/cli": "2.0.0-beta.48",
    "@vuepress/client": "2.0.0-beta.48",
    "oh-vue-icons": "^1.0.0-rc3",
    "rimraf": "^3.0.2",
    "sass-loader": "^13.0.0",
    "serve": "^13.0.2",
    "vue": "^3.2.36",
    "vuepress": "2.0.0-beta.48",
    "vuepress-theme-gungnir": "^2.0.0-alpha.25"
  },
  "engines": {
    "node": ">=14",
    "pnpm": ">=7"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "webpack"
      ]
    }
  }
}
