{
  "name": "alomerry-blog",
  "private": true,
  "packageManager": "pnpm@7.17.1",
  "author": "Alomerry Wu <alomerry.wu@gmail.com>",
  "description": "Alomerry Wu's blog, powered by VuePress 2, themed by Gungnir.",
  "license": "Apache-2.0",
  "repository": "https://github.com/alomerry/blog.git",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": ">=14",
    "pnpm": ">=7"
  },
  "scripts": {
    "clean": "pnpm --stream -r clean",
    "purge": "pnpm purge:packages && pnpm --filter blog blog:purge",
    "build": "pnpm clean && pnpm build:packages && pnpm blog:build && node scripts/postbuild.js",
    "purge:packages": "pnpm --stream -r purge",
    "build:packages": "pnpm --stream -r build && pnpm --parallel --stream -r copy",
    "blog:dev": "pnpm --filter blog blog:dev",
    "go": "pnpm install && pnpm blog:clean && pnpm blog:dev",
    "blog:build": "pnpm --filter blog blog:build",
    "blog:clean": "pnpm --filter blog clean",
    "postinstall": "node scripts/postinstall.js",
    "preview": "serve blog/.vuepress/dist",
    "prepare": "node scripts/prepare.js",
    "oss": "pnpm --filter blog blog:oss"
  },
  "devDependencies": {
    "@types/webpack-env": "^1.17.0",
    "cpx": "^1.5.0",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "serve": "^14.0.1",
    "typescript": "^4.8.3"
  },
  "pnpm": {
    "patchedDependencies": {
      "vuepress-theme-gungnir@2.0.0-alpha.26": "patches/vuepress-theme-gungnir@2.0.0-alpha.26.patch"
    }
  }
}
