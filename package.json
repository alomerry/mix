{
  "name": "blog",
  "version": "1.0.0",
  "private": true,
  "author": "Alomerry Wu <alomerry.wu@gmail.com>",
  "description": "Alomerry Wu's blog, powered by VuePress 2, themed by VuePress-Theme-Hope.",
  "license": "MIT",
  "type": "module",
  "repository": "https://github.com/alomerry/blog.git",
  "scripts": {
    "build": "node scripts/cmd.js setupI18n convertCDN downloadImportCode && vuepress build src",
    "clean-dev": "vuepress dev src --clean-cache",
    "dev": "vuepress dev src",
    "update-package": "pnpm dlx vp-update",
    "postbuild": "node scripts/cmd.js addUmamiTrace",
    "convert-cdn": "node scripts/cmd.js convertCDN",
    "auto-add-ioi": "node scripts/cmd.js autoGenIOISidebar",
    "download-import": "node scripts/cmd.js downloadImportCode",
    "oss": "./src/.vuepress/ossPusher --configPath core.toml",
    "split-fonts": "node scripts/cmd splitFonts modifySplitFontsCss"
  },
  "engines": {
    "node": ">=19",
    "pnpm": ">=8"
  },
  "devDependencies": {
    "@konghayao/cn-font-split": "3.4.2",
    "chalk": "^5.3.0",
    "request": "2.88.2"
  },
  "dependencies": {
    "@vue/repl": "^2.6.1",
    "@vuepress/client": "2.0.0-beta.67",
    "@vuepress/plugin-container": "2.0.0-beta.67",
    "@vuepress/shared": "2.0.0-beta.67",
    "@vuepress/utils": "2.0.0-beta.67",
    "flowchart.ts": "^1.0.1",
    "js-yaml": "4.1.0",
    "katex": "^0.16.9",
    "markdown-it": "13.0.1",
    "mathjax-full": "^3.2.2",
    "mermaid": "^10.6.0",
    "reveal.js": "^4.6.1",
    "sass-loader": "13.3.0",
    "vue": "^3.3.7",
    "vue-router": "^4.2.5",
    "vuepress": "2.0.0-beta.67",
    "vuepress-plugin-components": "2.0.0-beta.242",
    "vuepress-plugin-md-enhance": "2.0.0-beta.242",
    "vuepress-plugin-search-pro": "2.0.0-beta.242",
    "vuepress-shared": "2.0.0-beta.242",
    "vuepress-theme-hope": "2.0.0-beta.242"
  }
