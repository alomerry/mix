{
  "name": "alomerry-blog",
  "private": true,
  "description": "Alomerry Wu's blog, powered by VuePress 2, themed by Gungnir.",
  "license": "Apache-2.0",
  "author": "Alomerry Wu <alomerry@hotmail.com>",
  "scripts": {
    "clean": "rimraf blog/.vuepress/.temp blog/.vuepress/.cache dist",
    "dev": "vuepress dev blog --clean-cache",
    "build": "yarn clean && vuepress build blog --clean-cache --dest dist",
    "serve": "anywhere -h localhost -d dist"
  },
  "dependencies": {
    "@vuepress/bundler-vite": "2.0.0-beta.45",
    "@vuepress/cli": "2.0.0-beta.45",
    "@vuepress/client": "2.0.0-beta.45",
    "anywhere": "^1.6.0",
    "oh-vue-icons": "^1.0.0-rc3",
    "sass-loader": "^12.6.0",
    "rimraf": "^3.0.2",
    "vue": "^3.2.33",
    "vuepress": "2.0.0-beta.45",
    "vuepress-theme-gungnir": "2.0.0-alpha.23"
  }
}
